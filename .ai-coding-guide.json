{
  "domain": "general",
  "domains": {
    "primary": "general",
    "additional": []
  },
  "domainPriority": [
    "general"
  ],
  "constantResolution": {},
  "version": "1.0",
  "constants": {},
  "terms": {
    "entities": [],
    "properties": [],
    "actions": []
  },
  "naming": {
    "style": "camelCase",
    "booleanPrefix": [
      "is",
      "has",
      "should",
      "can"
    ],
    "asyncPrefix": [
      "fetch",
      "load",
      "save"
    ],
    "pluralizeCollections": true
  },
  "antiPatterns": {
    "forbiddenNames": [],
    "forbiddenTerms": []
  },
  "experimentalExternalConstants": false,
  "externalConstantsAllowlist": [],
  "architecture": {
    "fileStructure": {
      "pattern": "feature-based"
    },
    "maxFileLength": {
      "cli": 100,
      "command": 150,
      "util": 200,
      "generator": 250,
      "component": 300,
      "default": 250
    },
    "organization": {
      "commands": "lib/commands/{command}/",
      "utils": "lib/utils/",
      "generators": "lib/generators/",
      "tests": "tests/"
    },
    "functions": {
      "maxLength": 50,
      "maxComplexity": 10,
      "maxDepth": 4,
      "maxParams": 4,
      "maxStatements": 30,
      "singlePurpose": true,
      "preferPure": true
    },
    "patterns": {
      "cliStyle": "orchestration-shell",
      "errorHandling": "explicit",
      "asyncStyle": "async-await"
    }
  }
}
